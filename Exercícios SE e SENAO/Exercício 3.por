programa {
  funcao inicio() {
    real numero
    escreva("INFORME O NUMERO\n")
    leia(numero)
    se(numero<100){
      escreva("SEU NUMERO NÃO ESTA EM UM INTERVALO DE 100 E 200")
    }
    senao se(numero>=100){
      se(numero<=200){
        escreva("SEU NUMERO ESTA EM UM INTERVALO DE 100 E 200")
      }
      senao{
        escreva("SEU NUMERO NÃO ESTA EM UM INTERVALO DE 100 E 200")
      }
    }
   }





  }

