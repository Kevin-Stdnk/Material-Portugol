programa {
  funcao inicio() {
    real var1,var2
    escreva("INFORME O PRIMEIRO NUMERO \n")
    leia(var1)
    escreva("INFORME O SEGUNDO NUMERO \n")
    leia(var2)
    se(var1>var2){
      escreva("O PRIMEIRO NUMERO É MAIOR QUE O SEGUNDO")
    }
    se(var2>var1){
      escreva("O SEGUNDO NUMERO É MAIOR QUE O PRIMEIRO")
    }
    se(var1==var2){
      escreva("OS DOIS NUMERO SÃO IGUAIS")
    }
  }
}
